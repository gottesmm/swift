sil @test_swift_enum_1 : $@convention(thin) (@guaranteed Builtin.NativeObject) -> () {
bb0(%0 : $Builtin.NativeObject):
  %2 = enum $FakeOptional<Builtin.NativeObject>, #FakeOptional.Some!enumelt.1, %0 : $Builtin.NativeObject
  %3 = struct $FakeOptionalContainer<Builtin.NativeObject>(%2 : $FakeOptional<Builtin.NativeObject>)
  %4 = struct_extract %3 : $FakeOptionalContainer<Builtin.NativeObject>, #FakeOptionalContainer._value
  switch_enum %4 : $FakeOptional<Builtin.NativeObject>, case #FakeOptional.Some!enumelt.1: bb1, case #FakeOptional.None!enumelt: bb2

bb1(%5 : $Builtin.NativeObject):
  %6 = unchecked_ref_cast %5 : $Builtin.NativeObject to $E
  %7 = enum $FakeOptional<E>, #FakeOptional.Some!enumelt.1, %6 : $E
  br bb3(%7 : $FakeOptional<E>)

bb2:
  %8 = enum $FakeOptional<E>, #FakeOptional.None!enumelt
  br bb3(%8 : $FakeOptional<E>)

bb3(%9 : $FakeOptional<E>):
  release_value %9 : $FakeOptional<E>
  %10 = tuple()
  return %10 : $()
}

